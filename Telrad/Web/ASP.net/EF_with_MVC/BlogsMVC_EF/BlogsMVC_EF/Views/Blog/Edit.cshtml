@using BlogsMVC_EF.Models

@model Blog

@{ 
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<form method="post" action="/Blog/Edit">
    <input type="number" value="@Model.Id" name="id" readonly required /><br />
    <input type="text" name="title" value="@Model.Title" required /><br />
    <input type="submit" value="Submit" />
</form>
